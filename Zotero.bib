@article{kubin_evolution_1990,
	title = {Evolution of dislocation densities and the critical conditions for the {Portevin}-{Le} {Châtelier} effect},
	volume = {38},
	issn = {0956-7151},
	url = {http://www.sciencedirect.com/science/article/pii/0956715190900218},
	doi = {10.1016/0956-7151(90)90021-8},
	abstract = {A model is proposed for the critical strains associated with the Portevin-Le Châtelier effect (PLC) in terms of the strain dependence of the densities of mobile and forest dislocations. The classical critical condition for the onset of the PLC effect, viz. that of vanishing of the strain rate sensitivity of flow stress under the influence of dynamic strain aging is reexamined. The analysis takes into account the strain dependence of a key quantity: the elementary strain produced when all mobile dislocations perform a successful thermally activated step through the forest obstacles. This elementary strain is estimated by studying a system of coupled differential equations for the evolution of the two densities. Results are obtained in semi-quantitative form and compared with available data. It is shown that the following effects are consistently explained: the occurrence of critical strains for the onset and termination of jerky flow, occasional observation of two PLC regimes within the same deformation curve, the behaviour of the critical strains at high strain rates and low temperatures and, possibly, the particular behaviour exhibited by some alloys at low strain rates and high temperatures. Consequences for the “friction” and “forest” models of dynamic strain aging are discussed. Un modéle tenant compte de la variation avec la déformation des densités de dislocations mobiles et de la forêt est proposé pour les déformations critiques associées à l'effett Portevin-Le Châtelier (PLC). La condition critique classique pour l'application de l'effet PLC suivant laquelle la sensibilité à la vitesse de la contrainte d'écoulement s'annule sous l'effet du vieillissement dynamique est réexaminée. L'analyse est effectuée en fonction d'une quantité essentielle: la déformation élémentaire obtenue lorsque toutes les dislocations mobiles, effectuent un mouvement thermiquement acitivé à travers les obstacles de la forêt. Cette déformation élémentaire et sa dépendence en déformation sont calculées au moyen d'un système d'équations différentielles couplées pour lévolution des deux densités. Les résultats obtenus sous forme semi-quantitative sont comparés avec les données de la littérature. Les effects suivants peuvent alors être expliqués: l'existence de déformations critiques pour l'apparition et la disparition de la déformation instable, l'observation occasionnelle de deux régimes successifs d'effet PLC sur une même courbe de déformation, lévolution des déformations critiques à forte vitesse de déformation et à basse température, peut-être également le comportement particulier de certains alliages à faible vitesse et à haute température. Les conséquences concernant les modéles de “friction” et de “forêt” du vieillissement dynamique sont discutées. Es wird ein Modell zur Beschreibung der kritischen Dehnungen vorgestellt, die für den Portevin-Le Châtelieer-Effekt (PLC) charakteristisch sind. Dem Modell liegt die Dehnungs-abhängigkeit der Dichten von beweglichen und Waldversetzungen zugrunde. Die klassische kritische Bedingung für das Einssetzen des PLC-Effektes, die darin besteht, daβ die Geschwindigkeitsempfindlichkeit der Flieβspannung als Folge dynamischer Reckalterung gleich Null wird, wird überprüft. Hierbei wird der Analyse der Dehnungsabhängigkeit der Elementardehnung eine Schlüsselrolle beigemessen. Diese Gröβe wird als Dehnungsinkrement definiert, der durch einen thermisch aktivierten Schritt aller beweglichen Versetzungen im Hindernisfeld der Waldversetrzungen erzeugt wird. Diese Elementardehnung wird abgeschätz, indem ein Differentialgleichungssystem fü die beiden gekoppelten Versetzungsdichten untersucht wird. Die Ergebnisse sind in halbquantitativer Form präsentiert. Sie werden mit vorhandenen Meβdaten verglichen. Es wird gezeigt, daβ folgende Effekte schlüssig erklärt werden können: das Aufreten der kritischen Dehnungen, bei denen diskontinuierliches plastisches Flieβen einsetzt bzw. aufhört; vereinzelte Beobachtungen des Auftretens zweier PLC-Bereiche innerhalb einer und derselben Verformungskurve; das Verhalten der kritischen Dehnungen bei hohen Dehngeschwindigkeiten und niedrigen Temperaturen und—in bestimmten Legierungen—bei niedrigen Dehngeschwindigkeiten und hohen Temperaturen. Die Auswirkungen des Modells für das “Reibungs”-bzw. “Waldmodell” der dynamischen Reckalterung werden diskutiert.},
	number = {5},
	urldate = {2016-12-29TZ},
	journal = {Acta Metallurgica et Materialia},
	author = {Kubin, L. P. and Estrin, Y.},
