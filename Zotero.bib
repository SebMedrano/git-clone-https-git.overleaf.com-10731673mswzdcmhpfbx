@article{militzer_modelling_1994,
	title = {Modelling the effect of deformation-induced vacancies on segregation and precipitation},
	volume = {42},
	issn = {0956-7151},
	url = {http://www.sciencedirect.com/science/article/pii/0956715194900566},
	doi = {10.1016/0956-7151(94)90056-6},
	abstract = {The influence of deformation-induced excess vacancies on segregation and on precipitation is discussed within a phenomenological framework. During high temperature deformation, vacancies are mainly generated by the non-conservative movement of thermal jogs. Sinks for these vacancies are dislocations and grain boundaries. The excess vacancy concentration increases with strain rate and inverse temperature. Stronger effects are expected in f.c.c. materials because of their comparatively low diffusivities; in austenitic steels, the concentrations of deformation-induced vacancies are predicted to be significant, even at temperatures as high as 0.75 of the melting temperature. At the high strain rates ({\textgreater} 10 s−1) used in industrial rolling, the vacancy concentration may attain levels 2–3 orders-of-magnitude higher than the equilibrium one. One consequence of the presence of deformation-induced vacancies is the non-equilibrium segregation of boron observed e.g. in austenitic Nb-B steels, where it stimulates the nucleation of Nb borocarbonitrides.
L'influence de lacunes en excès induites par déformation sur la ségrégation et sur la précipitation est analysée dans la cadre d'une modèle phénoménologique. Lors de la déformation à haute température les lacunes sont surtout formées par le mouvement non conservatif de sauts de dislocation thermiques. L'annihilation de ces lacunes se produit au niveau des dislocations et joints de grain. La concentration de lacunes en excès augmente avec la vitesse de déformation et l'inverse de la temperature. Les effets sont plus importants dans les matériaux c.f.c., dû à leur taux de diffusion relativement faible. Dans les aciers austénitiques, des concentrations significatives de lacunes induites par déformation sont prédites, même à des températures aussi élevées que 0.75 du point de fusion. Pour les vitesses de déformation importantes utilisées dans les procédés industriels de laminage, la concentration en lacunes peut atteindre des ordres de grandeur de 2 à 3 fois plus élevés que ceux enregistrés à l'équilibre. Une conséquence de la formation de lacunes induite par déformation est la ségrégation hors équilibre de bore, observée par exemple dans les aciers austénitiques Nb-B, dans lesquels elle stimule la précipitation de Nb-borocarbonitrides.
Der Einflub verformungsinduzierter Überschubleerstellen auf das Segregationsund Ausscheidungsverhalten wird im Rahmen eines phänomenologischen Modells diskutiert. Während Warmverformung werden Leerstellen hauptsächlich durch die nichtkonservative Bewegung von thermischen Versetzungssprüngen erzeugt. Senken für diese Leerstellen sind Versetzungen und Korngrenzen. Die Konsentration der Überschubleerstellen steigt mit Verformungsgeschwindigkeit und inverser Temperatur an. Wegen der relativ geringen Diffusionstaten werden stärkere Effekte in k.f.z.-Materialien erwartet. Für austenitische Stähle wird abgeschätzt, dab die Konzentration verformungsinduzierter Leerstellen selbst noch bei Temperaturen von 0, 75 der Schmelztemperatur erheblich ist. Für hohe Verformungsgeschwindigkeiten ({\textgreater} 10 s−1), wie sie in industriellen Walzprozessen auftreten, kann die Leerstellenkonzentration zwei bis drei Gröbenordnungen über dem Gleichgewichtswert liegen. Eine Folge der Existenz verformungsinduzierter Leerstellen ist die Nichtgleichgewichtssegregation von Bor, die z.B. in austenitischen Nb-B-Stählen beobachtet wird, wo sie die Keimblildung von Nb-Borokarbonitriden beschleunigt.},
	number = {1},
	urldate = {2014-05-02TZ},
	journal = {Acta Metallurgica et Materialia},
	author = {Militzer, M. and Sun, W. P. and Jonas, J. J.},
	month = jan,
	year = {1994},
	pages = {133--141}
}

@article{matsukawa_situ_2012,
	series = {Microstructure {Properties} of {Irradiated} {Materials}},
	title = {In situ {TEM} study on elastic interaction between a prismatic loop and a gliding dislocation},
	volume = {425},
	issn = {0022-3115},
	url = {http://www.sciencedirect.com/science/article/pii/S0022311511010294},
	doi = {10.1016/j.jnucmat.2011.12.001},
	abstract = {In situ straining in a transmission electron microscope was performed in order to investigate dislocation interactions with a prismatic loop, which as a mobile obstacle is expected to be displaced by the strain-field of dislocation prior to physical contact. It was found that when a gliding dislocation approached a critical distance, the prismatic loop was certainly attracted to the dislocation. The captured loop disrupted the dislocation motion and was not dragged along with the mobile dislocation. Instead, the dislocation bypassed the loop via cross-slip to another slip plane with a resolved shear stress estimated to be 40\% lower than that of the original plane.},
	number = {1–3},
	urldate = {2014-04-27TZ},
	journal = {Journal of Nuclear Materials},
	author = {Matsukawa, Yoshitaka and Liu, Grace S.},
	month = jun,
	year = {2012},
	pages = {54--59}
}

@article{dollar_effect_1987,
	title = {The effect of grain size and strain on the tensile flow stress of quenched aluminum},
	volume = {35},
	issn = {0001-6160},
	url = {http://www.sciencedirect.com/science/article/pii/0001616087902306},
	doi = {10.1016/0001-6160(87)90230-6},
	abstract = {Tensile stress-strain data over the whole strain range were obtained at room temperature for slowly-cooled, for as-quenched and for quenched and aged pure aluminum. Transmission electron microscopy was performed for as-quenched and quenched and aged Al. The influence of grain size and strain on the strengthening of quenched and aged Al was analyzed in terms of the interaction of vacancies with grain boundaries, leading to different structure and different dislocation behavior in grain interiors and near grain boundaries. This approach gives a satisfactory quantitative description of the effect of grain size on the yield stress. The strengthening of as-quenched Al is also discussed.
Nous avons mesuré les différentes données contrainte-déformation relatives aux essais de traction réalisés dans la totalité du domaine de déformation, sur de l'aluminium pur

Für langsam abgekühltes, abgeschrecktes und abgeschrecktes und ausgelagertes reines Aluminium wurden die Spannungs-Dehnungskurven im gesamten Dehnungsbereich mit Zugverformung bei Raumtemperature ermittelt. An den abgeschreckten und den abgeschreckten und ausgelagerten Proben wurden elektronenmikroskopische Untersuchungen durchgefürt. Der Einfluβ von Korngröβe und Dehnung auf die Verfestigung des abgeschreckten und ausgelagerten Aluminiums wurde anhand der Wechselwirkung zwischen Leerstellen und Korngrenzen analysiert. Diese Wechselwirkung führt zu einem unterschiedlichen Versetzungsverhalten und damit zu einer unterschiedlichen Struktur im Korninneren und in der Nähe der Korngrenze. Dieses Vorgehen ergibt eine befriedigende quantitative Beschreibung des Einflusses der Korngröβe auf die Fleiβspannung. Die Verfestigung des abgeschreckten Aluminiums wird ebenfalls diskutiert.},
	number = {1},
	urldate = {2014-04-27TZ},
	journal = {Acta Metallurgica},
	author = {Dollar, M. and Thompson, A. W.},
	month = jan,
	year = {1987},
	pages = {227--235}
}

