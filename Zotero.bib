@incollection{humphreys_chapter_2004,
	address = {Oxford},
	title = {Chapter 2 - {The} {Deformed} {State}},
	isbn = {978-0-08-044164-1},
	url = {http://www.sciencedirect.com/science/article/pii/B9780080441641500062},
	abstract = {The aim of this chapter is to discuss the deformed state. The most important of the many changes associated with recrystallization and other annealing phenomena is the decrease in the density of dislocations. This chapter is concerned with dislocation accumulation rather than dislocation loss and increases in stored energy that are a result of deformation. The dislocations provide the driving force for the annealing phenomena. During deformation the microstructure of a metal changes in several ways. The first and the most obvious is that grains change their shape and there is a large increase in the total grain boundary area. The new grain boundary area has to be created during deformation and this is done by the incorporation of some of the dislocations that are continuously created during the deformation process. Another variation is the appearance of an internal structure within the grains. This, too, results from the accumulation of dislocations. The sum of the energy of all of the dislocations and new interfaces represents the stored energy of deformation. During deformation the orientations of single crystals and of the individual grains of a polycrystalline metal change relative to the direction of the applied stress.},
	urldate = {2014-05-22TZ},
	booktitle = {Recrystallization and {Related} {Annealing} {Phenomena} ({Second} {Edition})},
	publisher = {Elsevier},
	author = {Humphreys, F. J. and Hatherly, M.},
	editor = {Humphreys, F. J. and Hatherly, M.},
	year = {2004},
	pages = {11--II}
}

