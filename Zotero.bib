}

@article{zolotorevsky_study_2009,
	title = {Study of work hardening of quenched and naturally aged {Al}–{Mg} and {Al}–{Cu} alloys},
	volume = {502},
	issn = {0921-5093},
	url = {http://www.sciencedirect.com/science/article/pii/S0921509308012057},
	doi = {10.1016/j.msea.2008.10.010},
	abstract = {A comparative study on the work hardening of Al–Mg and Al–Cu alloys was carried out using a Kocks–Mecking–Estrin type analysis of stress–strain curves obtained in tension tests at constant loading rate. As a result of the analysis, dependencies of forest dislocation storage and dynamic recovery rates on the Mg and Cu concentration have been derived. The work hardening behavior and the microstructure formation in the Al–Mg and Al–Cu alloys were shown to be similar despite the opposite effects of Cu and Mg on stacking fault energy as well as the differences in solute atom size and friction stress. The influence of alloying on the work hardening peculiarities and the dislocation substructure evolution was discussed in connection with the effects of solute–dislocation interaction.},
	number = {1–2},
	urldate = {2016-12-28TZ},
	journal = {Materials Science and Engineering: A},
	author = {Zolotorevsky, N. Yu. and Solonin, A. N. and Churyumov, A. Yu. and Zolotorevsky, V. S.},
	month = feb,
	year = {2009},
	keywords = {Al–Cu alloys, Al–Mg alloys, Flow stress, Modeling, Work hardening},
	pages = {111--117}
}

@article{sandstrom_model_1975,
	title = {A model for hot working occurring by recrystallization},
	volume = {23},
	issn = {0001-6160},
	url = {http://www.sciencedirect.com/science/article/pii/0001616075901327},
	doi = {10.1016/0001-6160(75)90132-7},
	abstract = {A theoretical model is presented for hot working when dynamical recrystallization occurs. The workhardening and recovery of the dislocation structure as well as the recrystallization and the continuous grain growth are taken into account. The model is based on a description of the volume distributions of dislocations, which are produced as a result of the continuous recrystallization. Stress-strain curves are computed for iron and nickel and compared with experimental results of Luton and Sellars, and of Glover and Sellars. The model reproduces a number of features in the experimental curves as, for example, the flow stress oscillations at low strain rates, the distinct peak stress at high strain rates, the strain rate dependence of the peak stress and of the steady state stress, the strain rate dependence of the recrystalized grain size. The model suggests that the distinct peak stress at high strain rates is largely an effect of the variation in grain size during the dynamical recrystallization. On présente un modèle de la déformation à chaud lorsqu'il se produit une recristallisation dynamique. On tient compte de 1 écrouissage et de la restauration de la structure de dislocations, ainsi que de la recristallisation et de la croissance continue des grains. Le modèle repose sur une description de la répartition en volume des dislocations produites par la recristallisation continue. On compare les courbes contrainte-déformation calculées pour le nickel et le fer avec les résultats expérimentaux de Luton et Sellars, et de Glover et Sellars. Le modèle rend compte d'un certain nombre de caractéristiques des courbes expérimentales comme, par exemple, les oscillations de la contrainte de fluage aux faibles vitesses de déformation, le pic de contrainte distinct aux fortes vitesses de déformation, et l'influence de la vitesse de déformation sur le pic de contrainte, sur la contrainte en régime permanent et sur la taille des grains recristallisés. Le modèle suggère que le pic de contrainte, distinct aux fortes vitesses de déformation, provient en grande partie de la variation de la taille des grains au cours de la recristallisation dynamique. Es wird ein theoretisches Modell für die Warmverformung mit dynamischer Rekristallisation vorgesehlagen. Sowohl die Verfestigung und Erholung der Versetzungsstruktur als auch die Rekristallisation und das kontinuierliche Kornwachstum werden berücksichtigt. Das Modell basiert auf einer Beschreibung der räumlichen Verteilung der als Folge der kontinuierlichen Rekristallisation erzeugten Versetzungen. Verfestigungskurven werden für Eisen und Nickel berechnet und mit experimentellen Ergebnissen von Luton und Sellars und von Glover und Sellars verglichen. Das Modell liefert eine Reihe von Eigenschaften der experimentellen Kurven, wie, z.B. Flieβspannungsoszillationen bei kleinen Abgleitgeschwindigkeiten, die ausgeprägte Peak-Spannung bei groβen Abgleitgeschwindigkeiten, die Abhängigkeit der Peak-Spannung, der stationären Spannung und der rekristallisierten Korngröβe von der Abgleitgeschwindigkeit. Das Modell deutet darauf hin, daβ die Peak-Spannung bei hohen Abgleitgeschwindigkeiten hauptsächlich die Folge der während der dynamischen Rekristallisation variablen Korngröβe ist.},
	number = {3},
	urldate = {2016-12-15TZ},
	journal = {Acta Metallurgica},
	author = {Sandström, Rolf and Lagneborg, Rune},
	month = mar,
	year = {1975},
	pages = {387--398}
}

@article{estrin_local_1986,
	title = {Local strain hardening and nonuniformity of plastic deformation},
	volume = {34},
	issn = {0001-6160},
	url = {http://www.sciencedirect.com/science/article/pii/0001616086901483},
	doi = {10.1016/0001-6160(86)90148-3},
	abstract = {Local strain hardening in a cross-section of a deforming crystal has been related to the evolutionary behavior of the coupled densities of the mobile and (relatively immobile) forest dislocations. The local mechanical properties thus calculated differ from the bulk properties as obtained by the application of the Orowan relation on the bulk scale or by measuring the overall strain hardening in a macroscopic specimen. This difference is especially pronounced at small strains, and it is attributed to the inherently nonuniform character of plastic deformation by slip. The intrinsic stability of plastic deformation on the local scale has been examined with the help of standard methods of nonlinear analysis. In addition to such nonuniformities as yield points and the occurrence of necking (Considère instability), slip lines and slip bands also appear to be interprétable in terms of local instability of plastic flow. Le durcissement d'écrouissage d'un cristal en cours de déformation plastique est évalué à l'échelle locale d'une section de l'échantillon en fonction de l'évolution couplée de deux densités de dislocations: les dislocations mobiles et les dislocations de la forêt, relativement immobiles. Les propriétés mécaniques locales ainsi calculées diffèrent de celles prévues par l'application de la loi d'Orowan au matériau massif et des résultats expérimentaux obtenus sur échantillons macroscopiques. Cette différence, particulièrement marquée aux faibles déformations, est attribuée au caractère nonuniforme que présente systématiquement la déformation plastique par glissement. La stabilité intrinsèque de la déformation plastique est examinée à l'échelle locale à l'aide des méthodes courantes pour l'étude des systèmes non linéaires. Outre les phénomènes classiques tels que crochets de traction ou instabilité de Considère (striction), les lignes et bandes de glissement semblent également devoir être interprétées en termes d'une instabilité locale de l'écoulement plastique. Die lokale Verfestigung bei der Verformung in einem Kristallquerschnitt wurde mit dem Evolutionsverhalten zweier gekoppelter Versetzungsdichten in Verbindung gebracht: der Dichte von bewegten Versetzungen und der von (relativ unbewegten) Waldversetzungen. Die so berechneten lokalen mechanischen Eigenschaften unterscheiden sich von den makroskopischen Eigenschaften, wie sie durch Messungen an einer massiven Probe oder durch die Anwendung der Orowanschen Beziehung auf die gesamte Probe ermittelt werden. Diese Unterschiede, die besonders stark bei kleinen Dehnungen ausgeprägt sind, werden als Folge einer stets vorhandenen Ungleichmäβigkeit der plastischen Verformung mittels Gleitung angesehen. Die intrinsische Stabilität der lokalen plastischen Verformung wurde mit Hilfe von Standardmethoden der nichtlinearen Analysis untersucht. Auβer solchen Ungleichmäβigkeiten wie Streckgrenzen und Einschnürungen (Considère-Instabilität), scheinen auch Gleitlinien und Gleitbänder in den Kategorien der lokalen Instabilität interpretierbar zu sein.},
	number = {12},
	urldate = {2016-12-13TZ},
	journal = {Acta Metallurgica},
	author = {Estrin, Y. and Kubin, L. P.},
	month = dec,
	year = {1986},
	pages = {2455--2464}
}

@article{volin_annealing_1968,
	title = {Annealing kinetics of voids and the {Self}-diffusion coefficient in aluminum},
	volume = {25},
	issn = {1521-3951},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/pssb.19680250116/abstract},
	doi = {10.1002/pssb.19680250116},
	abstract = {The annealing kinetics of voids in 99.9999 wt\% pure aluminum were studied over the temperature range 85 to 209 °C in thin specimens by transmission electron microscopy. The isothermal shrinkage of individual voids was measured and interpreted on the basis of a self-diffusion annealing model. The self-diffusion coefficient of aluminum, as determined from the data, was given by Ds = 0.176 exp (-1.31 eV/kt) cm2s−1. The activation energy, Q = 1.31 eV, is significantly lower than the value Q = 1.48 eV determined by Lundy and Murdock at temperatures near the melting point in the only reported measurement by the radioactive tracer technique. Considerable evidence from a variety of sources is discussed which tends to support the present result.},
	language = {en},
	number = {1},
	urldate = {2016-12-12TZ},
	journal = {physica status solidi (b)},
	author = {Volin, T. E. and Balluffi, R. W.},
	month = jan,
	year = {1968},
	pages = {163--173}
}

@article{mordehai_introducing_2008,
	title = {Introducing dislocation climb by bulk diffusion in discrete dislocation dynamics},
	volume = {88},
	issn = {1478-6435},
	url = {http://www-tandfonline-com.ezproxy.library.ubc.ca/doi/abs/10.1080/14786430801992850},
	doi = {10.1080/14786430801992850},
	abstract = {We report a method to incorporate dislocation climb controlled by bulk diffusion in a three-dimensional discrete dislocation dynamics (DDD) simulation for fcc metals. In this model we couple the vacancy diffusion theory to the DDD in order to obtain the climb rate of the dislocation segments. The capability of the model to reproduce the motion of climbing dislocations is examined by calculating several test-cases of pure climb-related phenomena and comparing the results with existing analytical predictions and experimental observations. As test-cases, the DDD is used to study the activation of Bardeen–Herring sources upon the application of an external stress or under vacancy supersaturation. Loop shrinkage and expansion due to vacancy emission or absorption is shown to be well described by our model. In particular, the model naturally describes the coarsening of a population of loops having different sizes.},
	number = {6},
	urldate = {2016-12-09TZ},
	journal = {Philosophical Magazine},
	author = {Mordehai, Dan and Clouet, Emmanuel and Fivel, Marc and Verdier, Marc},
	month = feb,
	year = {2008},
	pages = {899--925}
}

@article{al-fadhalah_microplastic_2008,
	title = {Microplastic processes developed in pure {Ag} with mesoscale annealing twins},
	volume = {56},
	issn = {1359-6454},
	url = {http://www.sciencedirect.com/science/article/pii/S1359645408005478},
	doi = {10.1016/j.actamat.2008.07.050},
	abstract = {The impact of annealing twin boundaries with a high residual defect content on the mechanical response of polycrystalline fine- and coarse-grained (2 and 20 μm) silver was investigated through transmission electron microscopy and modeling. Besides an increase in the yield strength, the fine-grained material exhibited an inflection in the stress–strain curve after initial yield. Static and dynamic TEM studies revealed that the annealing twin boundaries acted as sources of perfect dislocations, partial dislocations and deformation twins; as barriers to the propagation of these dislocations; and as annihilation sites for dislocations. With increasing strain and as the twin boundaries were penetrated by dislocations, they contributed less to the overall mechanical properties. Based on these observations, equations for the evolution of mobile and forest dislocation densities are posed, depicting boundary sources and dislocation–dislocation interactions, respectively. The deformation response is modeled by computing the aggregate response of matrix–twin composite grains in the viscoplastic self-consistent scheme, which permits consideration of compatibility and equilibrium requirements across the twin boundaries. This work highlights the significant role boundaries play in generating the dislocations that control the macroscopic mechanical response.},
	number = {19},
	urldate = {2016-12-08TZ},
	journal = {Acta Materialia},
	author = {Al-Fadhalah, Khaled Jabr Hasan and Li, Chun-Ming and Beaudoin, A. J. and Korzekwa, D. A. and Robertson, I. M.},
	month = nov,
	year = {2008},
	keywords = {Plastic deformation, Silver, Tension test, Transmission electron microscopy},
	pages = {5764--5774}
}

