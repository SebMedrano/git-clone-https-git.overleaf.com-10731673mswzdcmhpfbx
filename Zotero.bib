	author = {Lam, Nghi Q. and Hoff, H. A. and Okamoto, P. R. and Siegel, R. W.},
	month = may,
	year = {1979},
	pages = {799--805}
}

@article{eikum_precipitation_1964,
	title = {Precipitation and dislocation nucleation in quench-aged {Al}-{Mg} alloys},
	volume = {12},
	issn = {0001-6160},
	url = {http://www.sciencedirect.com/science/article/pii/0001616064900264},
	doi = {10.1016/0001-6160(64)90026-4},
	abstract = {Small precipitates have been observed in thin foils prepared from bulk Al-5\% Mg and Al-6.5\% Mg alloys after quenching followed by aging at 100°C. The precipitates, concluded to be an intermediate form of Mg2Al3, will apparently nucleate only when a critical supersaturation of both Mg atoms and vacancies exists in the specimen. The vacancy denuded zone between colonies of prismatic dislocation loops and grain boundaries is one good example where the critical supersaturation can occur.

A high stress is associated with the precipitates, which can be relaxed by the punching out of rows of prismatic dislocation loops. This has been observed directly in the electron microscope. A comparison of the experimental results with the theory of Bullough and Newman enables the critical shear stress for prismatic glide to be estimated. The value obtained is 3.4 × 10−4G.

Some general observations on the nature of the precipitation and the generation of dislocation loops are discussed.},
	number = {5},
	urldate = {2014-09-30TZ},
	journal = {Acta Metallurgica},
	author = {Eikum, A and Thomas, G},
	month = may,
	year = {1964},
	pages = {537--545}
}

@article{panseri_evidence_1963,
	title = {Evidence for the interaction between {Mg} atoms and vacancies in {Al}-{Zn} 10\%-{Mg} 0.1\% alloy},
	volume = {11},
	issn = {0001-6160},
	url = {http://www.sciencedirect.com/science/article/pii/0001616063900919},
	doi = {10.1016/0001-6160(63)90091-9},
	abstract = {A resistometric investigation of the influence of a small addition of Mg (0.1\%) on the features of the pre-precipitation phenomenon in the Al-Zn 10\% alloy has been carried out following the same experimental scheme employed in a previous research on the binary alloy.

The results have shown that whereas the influence of Mg on the general pattern of pre-precipitation is negligible, all the kinetic behaviour of the process in radically changed by the presence of Mg, as the dependence on quenching and aging temperatures and the tune of complete reaction.

The results have been interpreted in a very consistent way based on the following points: 1.
(1) a strong binding energy exists between Mg atoms and vacancies in the ternary alloy so that all vacancies are coupled to Mg atoms;
2.
(2) Mg-vacancy couples can move easily at room temperature so that diffusion of Zn atoms in the ternary alloy occurs via Mg-vacancy couples.
The value of the binding energy results 0.54 ± 0.08 eV although a slightly lower value may also be consistent with the above interpretation.},
	number = {6},
	urldate = {2014-09-30TZ},
	journal = {Acta Metallurgica},
	author = {Panseri, C and Federighi, T},
	month = jun,
	year = {1963},
	pages = {575--584}
}

