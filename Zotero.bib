@article{gomiero_microstructure_1992,
	title = {Microstructure and mechanical properties of a 2091 {AlLi} alloy—{I}. {Microstructure} investigated by {SAXS} and {TEM}},
	volume = {40},
	issn = {0956-7151},
	url = {http://www.sciencedirect.com/science/article/pii/095671519290027C},
	doi = {10.1016/0956-7151(92)90027-C},
	abstract = {The microstructure of the 2091 alloy (AlLiCuMgZr) alloy has been studied and compared to that of two simpler AlLi and AlCuMg alloys. The SAXS, in situ SAXS and TEM techniques have been used for heat treatments at room temperature and at 150°C. The time evolution of the size and of the volume fraction of the δ′ precipitates and of the Cu-rich GPB zones have been determined. The δ′ precipitates are shown to follow a classical Lifschitz-Slyosov-Wagner law. Their interfacial energy has been estimated. S and S′ precipitates have also been characterized, respectively on grain boundaries and on helical dislocations, by TEM. La microstructure d'un alliage 2091(AlLiCuMgZr) a été étudiée et comparée à celle de deux alliages plus simples: AlLi et AlCuMg. Les techniques de diffusion X aux petits angles, conventionnelles et in situ, ainsi que la microscopie électronique à transmission ont été utilisées pour caractériser le mûrissement à l'ambiante et les revenus à 150°C. L'évolution temporelle de la taille et de la fraction volumique des précipités δ′ et des zones GPB (riches en cuivre) a été déterminée. Les précipités δ′ suivent une loi classique de Lifschitz-Slyosov-Wagner. Leur énergie d'interface a été estimée. Des précipités S et S′ ont été aussi caractérisés respectivement sur les joints de grain et sur les dislocations hélicoïdales par MET. Die Mikrostruktur der Legierung 2091 (AlLiCuMgZr) wird untersucht und verglichen mit derjenigen der einfacheren Legierungen AlLi and AlCuMg. Die Verfahren SAXS, in-situ-SAXS und TEM werden für die Wärmebehandlung bei Raumtemperatur und bein 150°C benutzt. Die zeitliche Entwicklung der Gröβe und der Volumanteile der δ′-Ausscheidungen und der Cu-reichen Guinier-Preston-Zonen wird bestimmt. Die δ′-Ausscheidungen folgen einem klassichen Lifschitz-Slyosov-Wagner-Gesetz. Deren Grenzflächenenergie wird abgeschätzt. S-Ausscheidungen an Korngrenzen und S′-Ausscheidungen an schraubenförmigen Versetzungslinien werden auβerdem mit TEM charakterisiert.},
	number = {4},
	urldate = {2016-03-29TZ},
	journal = {Acta Metallurgica et Materialia},
	author = {Gomiero, P and Livet, F and Brechet, Y and Louchet, F},
	month = apr,
	year = {1992},
	pages = {847--855}
}

@article{thomson_small-angle_1999,
	title = {Small-angle scattering by dislocations},
	volume = {55},
	issn = {1600-5724},
	url = {http://onlinelibrary.wiley.com.ezproxy.library.ubc.ca/doi/10.1107/S010876739801071X/abstract},
	doi = {10.1107/S010876739801071X},
	abstract = {It is shown that the small-angle scattering of X-rays or neutrons by dislocations within a deformed metal, which are partially ordered into wall-like structures, is characterized by several factors. Principally these are associated with: (i) a single dislocation or dipole; (ii) the dislocation configuration in the plane of the wall; and (iii) the distribution of dislocations across the wall thickness. With the assumption of isotropic elasticity, small-angle scattering will be sensitive only to the edge components of the dislocations. The scattered intensity is dominated by scattering from dislocations that lie perpendicular to the scattering vector, , and reaches a maximum when  is normal to the slip plane of these dislocations. Above a particular , the scattered intensity is sensitive only to the total edge dislocation content of the scattering dislocations (i.e. scattering is incoherent), while, below this value, the scattering is dominated by how the dislocations are distributed in walls. For walls normal to their slip planes, the configuration factor will reflect the dislocation distribution in the plane of the wall, while, for walls parallel to their slip planes, the distribution in the thickness direction will be visible. Therefore, even though a deformed material is composed of complicated dislocation structures, only those segments conforming to these rather strict prescriptions will be singled out for scattering, and, by adjusting the beam/slip system geometry, many parameters of the microstructure can be determined experimentally.},
	language = {en},
	number = {3},
	urldate = {2016-03-29TZ},
	journal = {Acta Crystallographica Section A},
	author = {Thomson, Robb and Levine, L. E. and Long, G. G.},
	month = may,
	year = {1999},
	pages = {433--447}
}

