@article{zhao_role_2014,
	title = {The role of grain boundary plane orientation in the β phase precipitation of an {Al}–{Mg} alloy},
	volume = {89},
	issn = {1359-6462},
	url = {http://www.sciencedirect.com/science/article/pii/S1359646214002772},
